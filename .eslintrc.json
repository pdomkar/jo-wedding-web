{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "standard"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            0
        ],
        "space-before-function-paren": [
            0
        ],
        "no-undef": [
            0
        ],
        "no-unused-vars": [
            0
        ]
    }
}